- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\n\nclass blk(gr.sync_block):\n\
      \    def __init__(self):  # solo argumentos por defecto aqu\xED\n        gr.sync_block.__init__(\n\
      \            self,\n            name='e_Diff',        # nombre que aparecer\xE1\
      \ en GRC\n            in_sig=[np.float32],  # se\xF1al de entrada\n        \
      \    out_sig=[np.float32]  # se\xF1al de salida\n        )\n        # Guardar\
      \ la \xFAltima muestra anterior\n        self.prev_sample = 0.0\n\n    def work(self,\
      \ input_items, output_items):\n        x = input_items[0]   # Se\xF1al de entrada\n\
      \        y0 = output_items[0] # Se\xF1al de salida (diferenciada)\n\n      \
      \  # Crear array de salida con la misma longitud que x\n        y = np.empty_like(x)\n\
      \n        # Primera diferencia usa la muestra previa guardada\n        y[0]\
      \ = x[0] - self.prev_sample\n\n        # Diferencias internas\n        y[1:]\
      \ = x[1:] - x[:-1]\n\n        # Actualizar la \xFAltima muestra para la pr\xF3\
      xima llamada\n        self.prev_sample = x[-1]\n\n        # Copiar a salida\n\
      \        y0[:] = y\n\n        return len(y0)\n"
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: ('e_Diff', 'blk', [], [('0', 'float', 1)], [('0', 'float', 1)], '',
      [])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [480, 336.0]
    rotation: 0
    state: enabled
- name: epy_block_1
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\n\nclass blk(gr.sync_block):\n\
      \    def __init__(self):  # solo argumentos por defecto aqu\xED\n        gr.sync_block.__init__(\n\
      \            self,\n            name='e_Acum',        # nombre que aparece en\
      \ GRC\n            in_sig=[np.float32],  # se\xF1al de entrada\n           \
      \ out_sig=[np.float32]  # se\xF1al de salida\n        )\n\n    def work(self,\
      \ input_items, output_items):\n        x = input_items[0]   # Se\xF1al de entrada\n\
      \        y0 = output_items[0] # Se\xF1al de salida acumulada\n        y0[:]\
      \ = np.cumsum(x) # Acumulador de la se\xF1al\n        return len(y0)       #\
      \ Devuelve la longitud procesada"
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: ('e_Acum', 'blk', [], [('0', 'float', 1)], [('0', 'float', 1)], '',
      [])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [480, 280.0]
    rotation: 0
    state: disabled
